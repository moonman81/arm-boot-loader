@prefix : <http://example.org/arm-boot-master-links#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .

# Master Linking Ontology for ARM Boot Ontologies
<http://example.org/arm-boot-master-links> a owl:Ontology ;
  rdfs:label "ARM Boot Master Links Ontology" .

# Classes
:Ontology a owl:Class ;
  rdfs:label "Ontology" ;
  rdfs:comment "An ontology in the collection" .

:Link a owl:Class ;
  rdfs:label "Link" ;
  rdfs:comment "Link between ontologies" .

# Properties
:relatedTo a owl:ObjectProperty ;
  rdfs:domain :Ontology ;
  rdfs:range :Ontology ;
  rdfs:label "related To" .

:linkType a owl:DatatypeProperty ;
  rdfs:domain :Link ;
  rdfs:range xsd:string ;
  rdfs:label "link Type" .

:ontologyURI a owl:DatatypeProperty ;
  rdfs:domain :Ontology ;
  rdfs:range xsd:string ;
  rdfs:label "ontology URI" .

:description a owl:DatatypeProperty ;
  rdfs:domain owl:Thing ;
  rdfs:range xsd:string ;
  rdfs:label "description" .

# PROV-O Instances
:MasterLinksOntology a prov:Entity ;
  prov:wasGeneratedBy :CreationActivity ;
  prov:wasAttributedTo :OpenCodeAgent ;
  rdfs:label "Master Links Ontology" .

:CreationActivity a prov:Activity ;
  prov:startedAtTime "2025-01-01T00:00:00Z"^^xsd:dateTime ;
  prov:endedAtTime "2025-01-01T01:00:00Z"^^xsd:dateTime ;
  rdfs:label "Ontology Creation" .

:OpenCodeAgent a prov:Agent ;
  rdfs:label "OpenCode AI Assistant" .

# Ontology Instances
:BootloadingOntology a :Ontology ;
  :ontologyURI "http://example.org/arm-bootloading" ;
  :description "Basic ARM bootloading concepts" .

:BootProcessOntology a :Ontology ;
  :ontologyURI "http://example.org/arm-boot-process" ;
  :description "Boot process stages" .

:RequirementsOntology a :Ontology ;
